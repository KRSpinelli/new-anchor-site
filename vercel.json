{
    "buildCommand": "npm run build",
    "outputDirectory": ".",
    "cleanUrls": true,
    "trailingSlash": false,
    "rewrites": [
        {
            "source": "/projects/parametric-particles",
            "destination": "https://parametric-particles.vercel.app/projects/parametric-particles"
        },
        {
            "source": "/projects/parametric-particles/:path*",
            "destination": "https://parametric-particles.vercel.app/projects/parametric-particles/:path*"
        }
    ]
}